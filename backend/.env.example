# Local development defaults for the Mister DJ backend.
# Copy to `.env` and adjust as needed when running the API locally.

# Core services
NODE_ENV=development
SERVICE_NAME=mr-dj-api
HOST=0.0.0.0
PORT=3000
LOG_FORMAT=pretty

# Database & storage
DATABASE_URL=postgres://postgres:postgres@localhost:5432/mr_dj
REDIS_URL=redis://localhost:6379/0
REDIS_TLS=false

# RentGuy integration
RENTGUY_API_BASE_URL=http://localhost:5050
RENTGUY_API_KEY=mock-api-key
RENTGUY_WORKSPACE_ID=workspace-1
RENTGUY_TIMEOUT_MS=5000

# Sevensa & automation flows
SEVENSA_SUBMIT_URL=https://sevensa.test/forms/submit
N8N_PERSONALIZATION_WEBHOOK_URL=https://n8n.test/webhook/personalization

# SEO automation defaults
SEO_AUTOMATION_API_URL=https://seo-automation.test/api
SEO_AUTOMATION_API_KEY=automation-test-key
SEO_AUTOMATION_KEYWORDSET_ID=keyword-set-demo

# City automation LLM configuration
CITY_AUTOMATION_LLM_PROVIDER=openai
CITY_AUTOMATION_LLM_MODEL=gpt-4.1-mini
CITY_AUTOMATION_LLM_API_KEY=test-llm-key

# Optional fallbacks
OPENAI_API_KEY=
PGSSLMODE=prefer
CORS_PUBLIC_ORIGINS=http://localhost:5173
CONFIG_DASHBOARD_ENABLED=false

# Mock + telemetry helpers
RENTGUY_MOCK_PORT=5050
RENTGUY_MOCK_HOST=127.0.0.1
RENTGUY_MOCK_API_KEY=mock-api-key
RENTGUY_MOCK_WORKSPACE_ID=workspace-1
